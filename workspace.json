{
  "version": 1,
  "projects": {
    "cypress-harness": {
      "root": "packages/cypress-harness",
      "sourceRoot": "packages/cypress-harness/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/cypress-harness/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cypress-harness/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/cypress-harness",
            "tsConfig": "packages/cypress-harness/tsconfig.lib.json",
            "packageJson": "packages/cypress-harness/package.json",
            "main": "packages/cypress-harness/src/index.ts",
            "assets": [
              "packages/cypress-harness/*.md"
            ]
          }
        },
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "cypress-harness-e2e": {
      "root": "packages/cypress-harness-e2e",
      "sourceRoot": "packages/cypress-harness-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/cypress-harness-e2e/cypress.json",
            "tsConfig": "packages/cypress-harness-e2e/tsconfig.e2e.json",
            "devServerTarget": ""
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/cypress-harness-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "cypress-mount": {
      "root": "packages/cypress-mount",
      "sourceRoot": "packages/cypress-mount/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/cypress-mount/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cypress-mount/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/cypress-mount",
            "tsConfig": "packages/cypress-mount/tsconfig.lib.json",
            "packageJson": "packages/cypress-mount/package.json",
            "main": "packages/cypress-mount/src/index.ts",
            "assets": [
              "packages/cypress-mount/*.md"
            ]
          }
        },
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "cypress-harness-sandbox-e2e": {
      "root": "packages/cypress-harness-sandbox-e2e",
      "sourceRoot": "packages/cypress-harness-sandbox-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "packages/cypress-harness-sandbox-e2e/cypress.json",
            "tsConfig": "packages/cypress-harness-sandbox-e2e/tsconfig.e2e.json",
            "devServerTarget": "cypress-sandbox:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cypress-sandbox:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/cypress-harness-sandbox-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/cypress-harness-sandbox-e2e/**/*"
            ]
          }
        }
      }
    },
    "cypress-sandbox": {
      "projectType": "application",
      "root": "packages/cypress-sandbox",
      "sourceRoot": "packages/cypress-sandbox/src",
      "prefix": "test-utils",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/packages/cypress-sandbox",
            "index": "packages/cypress-sandbox/src/index.html",
            "main": "packages/cypress-sandbox/src/main.ts",
            "polyfills": "packages/cypress-sandbox/src/polyfills.ts",
            "tsConfig": "packages/cypress-sandbox/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/cypress-sandbox/src/favicon.ico",
              "packages/cypress-sandbox/src/assets"
            ],
            "styles": [
              "packages/cypress-sandbox/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/cypress-sandbox/src/environments/environment.ts",
                  "with": "packages/cypress-sandbox/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "cypress-sandbox:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cypress-sandbox:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cypress-sandbox:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/cypress-sandbox/tsconfig.app.json",
              "packages/cypress-sandbox/tsconfig.spec.json",
              "packages/cypress-sandbox/tsconfig.editor.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!packages/cypress-sandbox/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/cypress-sandbox/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "cypress-harness",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  }
}
